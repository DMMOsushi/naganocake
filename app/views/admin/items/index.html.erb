<thead>
 <tr>
   <th>商品ID</th>
   <th>商品名</th>
   <th>税抜価格</th>
   <th>ジャンル</th>
   <th>販売ステータス</th>
 </tr>
</thead>
<tbody>
  <% @items.each do |item|%>
   <tr>
     <td><%= item.id %></td>
     <td><%= item.name %></td>
     <td><%= item.tax_excluded_price %><td>
     <td><%= item.category.name %></td>
     <td><%= item.sale_status %></td>
   </tr>
  <% end %>
</tbody>